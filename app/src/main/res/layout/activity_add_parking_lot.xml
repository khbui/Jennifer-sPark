<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jennifers.jenniferspark.AddParkingLot"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            tools:layout_editor_absoluteY="8dp"
            tools:layout_editor_absoluteX="8dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="Title"
                tools:layout_editor_absoluteY="27dp"
                tools:layout_editor_absoluteX="8dp" />

            <EditText
                android:id="@+id/parkinglottitletv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                tools:layout_editor_absoluteX="7dp"
                tools:layout_editor_absoluteY="43dp" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:hint="Address"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="86dp" />

            <EditText
                android:id="@+id/parkinglotaddresstv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                tools:layout_editor_absoluteX="8dp"
                tools:layout_editor_absoluteY="102dp" />
            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="City"
                tools:layout_editor_absoluteX="7dp"
                tools:layout_editor_absoluteY="145dp" />

            <EditText
                android:id="@+id/parkinglotcitytv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                tools:layout_editor_absoluteX="7dp"
                tools:layout_editor_absoluteY="163dp" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="State"
                tools:layout_editor_absoluteX="7dp"
                tools:layout_editor_absoluteY="205dp" />

            <EditText
                android:id="@+id/parkinglotstatetv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                tools:layout_editor_absoluteX="7dp"
                tools:layout_editor_absoluteY="222dp" />
            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Zipcode"
                tools:layout_editor_absoluteX="7dp"
                tools:layout_editor_absoluteY="265dp" />

            <EditText
                android:id="@+id/parkinglotziptv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                tools:layout_editor_absoluteX="7dp"
                tools:layout_editor_absoluteY="282dp" />
            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Description"
                tools:layout_editor_absoluteX="7dp"
                tools:layout_editor_absoluteY="325dp" />

            <EditText
                android:id="@+id/parkingdesctv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textPersonName"
                tools:layout_editor_absoluteX="7dp"
                tools:layout_editor_absoluteY="342dp" />
            <Button
                android:id="@+id/addparkinglotbtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Add"
                tools:layout_editor_absoluteX="148dp"
                tools:layout_editor_absoluteY="433dp" />
            <Button
                android:id="@+id/addparkinglotbckbtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Back"
                tools:layout_editor_absoluteX="148dp"
                tools:layout_editor_absoluteY="433dp" />
        </LinearLayout>
    </ScrollView>
</android.support.constraint.ConstraintLayout>
